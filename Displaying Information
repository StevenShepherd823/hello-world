
            string firstName = "";
            string lastName = "";
            int age = 0;
            string street = "";
            string city = "";
            string country = "";
            DateTime birthDate;

            // Assing some values
            firstName = "Steven";
            lastName = "Shepherd";
            age = 39;
            street = "Your Address";
            city = "Seattle";
            country = "USA";
            birthDate = new DateTime(1980, 01, 01);

            // output to console window

            //use simple output with just variable name
            Console.WriteLine("Student Information");
            Console.WriteLine("{0} {1} {2} {3} {4} {5}",firstName, lastName, birthDate, street, city, country);
            Console.ReadLine();
